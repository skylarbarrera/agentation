{
  "name": "@agentation/plugin-reanimated",
  "version": "0.1.0",
  "description": "Reanimated animation state plugin for Agentation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "agentation-rn": ">=0.1.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "reanimated-pause-state": ">=0.1.0"
  },
  "devDependencies": {
    "agentation-rn": "workspace:*",
    "reanimated-pause-state": "link:../../../reanimated-pause-state",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react-native",
    "reanimated",
    "agentation",
    "animation",
    "plugin"
  ],
  "author": "Skylar Barrera",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarbarrera/agentation.git",
    "directory": "plugins/plugin-reanimated"
  }
}
